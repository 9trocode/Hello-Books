  },
  "engines": {
    "node": "6.11.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nosisky/Hello-Books.git"
  },
  "author": "Abdulrasaq Nasirudeen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nosisky/Hello-Books/issues"
  },
  "homepage": "https://github.com/nosisky/Hello-Books#readme",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "dotenv": "^4.0.0",
    "eslint": "^4.3.0",
    "eslint-config": "^0.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
