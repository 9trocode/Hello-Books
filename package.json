{
  "name": "hello-books",
  "version": "1.0.0",
  "description": "Hello book is a simple library management system built with reactJS",
  "main": "server.js",
  "scripts": {
    "start": "nodemon --exec babel-node server.js",
    "test": "SET NODE_ENV=test&& mocha --timeout 40000 --compilers js:babel-core/register test ",
    "tests": "NODE_ENV=test&& mocha --timeout 40000 --compilers js:babel-core/register test ",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
